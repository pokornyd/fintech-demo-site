{	"compilerOptions": {		"allowJs": false,		"allowUnreachableCode": false,		"allowUnusedLabels": false,		"esModuleInterop": true,		"forceConsistentCasingInFileNames": true,		"incremental": true,		"jsx": "react",		"lib": [			"dom",			"scripthost",			"es7"		],		"noFallthroughCasesInSwitch": true,		"noImplicitReturns": true,		"noImplicitThis": true,		"noUnusedLocals": true,		"noUnusedParameters": true,		"skipLibCheck": true,		"sourceMap": true,		"strictBindCallApply": true,		"strictNullChecks": true,		"target": "es5"	}}